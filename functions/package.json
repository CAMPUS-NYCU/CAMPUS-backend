{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "serve": "firebase serve --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "localstart": "nodemon localIndex.js",
    "gen_docs": "npx jsdoc2md -f ./src/* ./src/datasources/* localIndex.js > ../docs/graphql.md",
    "serve_docs": "npx docsify serve ../docs",
    "firestore": "firebase emulators:start --only firestore",
    "test": "jest --watchAll"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "apollo-datasource": "^0.6.3",
    "apollo-server": "^2.16.0",
    "apollo-server-express": "^2.16.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase": "^7.16.1",
    "firebase-admin": "^8.6.0",
    "firebase-functions": "^3.8.0",
    "geofirestore": "^3.3.1",
    "graphql": "^14.5.8",
    "graphql-resolvers": "^0.4.2",
    "graphql-tools": "^4.0.6",
    "graphql-voyager": "^1.0.0-rc.29",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@firebase/testing": "^0.20.7",
    "@types/jest": "^26.0.7",
    "apollo-server-testing": "^2.16.0",
    "cz-conventional-changelog": "^3.2.0",
    "docsify-cli": "^4.4.1",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jsdoc-to-markdown": "^6.0.1",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
