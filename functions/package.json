{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "serve": "firebase serve --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "localstart": "nodemon localIndex.js",
    "gen_docs": "npx jsdoc2md -f ./src/* ./src/datasources/* localIndex.js > ../docs/graphql.md",
    "serve_docs": "npx docsify serve ../docs",
    "firestore": "firebase emulators:start --only firestore",
    "test": "jest --watchAll --runInBand --detectOpenHandles"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.3.0",
    "apollo-datasource": "^0.7.2",
    "apollo-server": "^2.17.0",
    "apollo-server-express": "^2.17.0",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase": "^7.19.1",
    "firebase-admin": "^9.1.1",
    "firebase-functions": "^3.11.0",
    "geofirestore": "^4.3.0",
    "google-spreadsheet": "^2.0.6",
    "graphql": "^15.3.0",
    "graphql-resolvers": "^0.4.2",
    "graphql-tools": "^6.2.0",
    "graphql-voyager": "^1.0.0-rc.29",
    "line-reader": "^0.4.0",
    "linebot": "^1.6.0",
    "lodash": "^4.17.20",
    "querystring": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "sharp": "^0.26.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@firebase/testing": "^0.20.11",
    "@types/jest": "^26.0.13",
    "apollo-server-testing": "^2.17.0",
    "cz-conventional-changelog": "^3.3.0",
    "docsify-cli": "^4.4.1",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "graphql-tag": "^2.11.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "jsdoc-to-markdown": "^6.0.1",
    "lint-staged": "^10.2.13",
    "prettier": "^2.1.1"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
